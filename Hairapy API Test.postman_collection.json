		{
			"name": "Post Predict",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"const responseJSON = pm.response.json();\r",
							"const responseData = responseJSON.data;\r",
							"\r",
							"pm.test('Route Loaded', ()=>{\r",
							"    pm.expect(pm.response.code).to.equals(200);\r",
							"})\r",
							"\r",
							"pm.test('Route returns an Object', ()=>{\r",
							"    pm.expect(responseJSON).to.be.an('object');\r",
							"})\r",
							"\r",
							"pm.test('Route returns with Prediction Result', ()=>{\r",
							"    \r",
							"    pm.expect(responseData).to.ownProperty('result');\r",
							"    pm.expect(responseData.result).to.be.an('string');\r",
							"})\r",
							"\r",
							"pm.test('Route Returns with Recomendation Object', ()=>{\r",
							"    pm.expect(responseData).to.ownProperty('recomendations');\r",
							"\r",
							"    if(responseJSON.recomendations){\r",
							"        const recomendations = responseData.recomendations;\r",
							"        pm.expect(recomendations).to.be.an('array');\r",
							"    }\r",
							"})"
						],
						"type": "text/javascript",
						"packages": {}
					}
				}
			],
